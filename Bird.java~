public class Bird {
 private String size=" ";
 public String color=" ";
 public Bird(){
 }
 public Bird(String v){
   size=v;
 
}

 public Bird(String c,String s){
   size=s;
 c=color;
}
public void fly(){
  if(  color!=" " && size== " "){
 System.out.println(size+"bird*is*flyin*");
}
  else{
   System.out.println(size+color+"Bird*is*flyin*"); 
    
  }
  
  
}
  
  
  
  
  
  
}


